package uts;

/**
 *
 * @author Fikri
 */
public class Person {
    String nama ;
    int age ;
    
}


/*
 * To change this license header, choose License Headers in Project Properties.
 * To change this template file, choose Tools | Templates
 * and open the template in the editor.
 */
package uts;

/**
 *
 * @author Fikri
 */
public class PersonBeraksi {
    public static void main (String[]args){
        Person orangku = new Person();
        Person kuy = new Person();
        
        kuy.age = 52;
        kuy.nama = "Mary Sharp";
        
        orangku.nama = "Joe Smith";
        orangku.age = 24;
        
        System.out.println("Nama : " + orangku.nama);
        System.out.println("Age : " + orangku.age);
        System.out.println("Nama : " + kuy.nama);
        System.out.println("Nama : " + kuy.age);
    }
}
