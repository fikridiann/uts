package uts;

public class Motor {
    String tipe;
    int tahun;
    
    void printMotor(){
        System.out.println("Type Motor : "+ tipe);
        System.out.println("Tahun : "+ tahun);
    }
}



package uts;

public class MotorBeraksi {
    public static void main(String[]args){
        Motor motorku = new Motor();
        
        motorku.tipe = "Beat";
        motorku.tahun = 2018;
        motorku.printMotor();
    }
}
